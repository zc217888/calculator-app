{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/calculate.ts": {
      "runtime": "@vercel/node@2.15.10"
    },
    "api/validate.ts": {
      "runtime": "@vercel/node@2.15.10"
    },
    "api/history.ts": {
      "runtime": "@vercel/node@2.15.10"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}